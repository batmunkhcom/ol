{
    "require": {
        "bzikarsky/gelf-php": "*",
        "klein/klein": "*",
        "predis/predis": "dev-master",
        "videlalvaro/php-amqplib": "*",
        "basement/basement": "0.3.0"
    },
    "autoload": {
        "classmap": [
            "functions/"
        ],
        "exclude": [
            "test"
        ],
        "psr-0": {
            "": ["User/", "Friend/", "Ol/", "Core/", "DB/"]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nrk/predis"
        }
    ],
    "minimum-stability": "stable"
}
